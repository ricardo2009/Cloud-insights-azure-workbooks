          {
            "type": 12,
            "content": {
              "version": "NotebookGroup/1.0",
              "groupType": "editable",
              "items": [
                {
                  "type": 3,
                  "content": {
                    "version": "KqlItem/1.0",
                    "query": "// Data Factory Pipeline Runs Analysis\nADFPipelineRun\n| where TimeGenerated {TimeRange}\n| extend \n    Duration = datetime_diff('minute', End, Start),\n    Status = case(\n        Status == \"Succeeded\", \"‚úÖ Success\",\n        Status == \"Failed\", \"‚ùå Failed\",\n        Status == \"InProgress\", \"üîÑ Running\",\n        Status == \"Cancelled\", \"üö´ Cancelled\",\n        \"‚ùì Unknown\"\n    )\n| summarize \n    TotalRuns = count(),\n    SuccessRuns = countif(Status == \"‚úÖ Success\"),\n    FailedRuns = countif(Status == \"‚ùå Failed\"),\n    AvgDuration = avg(Duration),\n    MaxDuration = max(Duration)\n    by bin(TimeGenerated, 1h), PipelineName\n| extend SuccessRate = round(todouble(SuccessRuns) / todouble(TotalRuns) * 100, 2)\n| project \n    TimeGenerated,\n    ['üè≠ Pipeline'] = PipelineName,\n    ['üìä Total Runs'] = TotalRuns,\n    ['‚úÖ Success'] = SuccessRuns,\n    ['‚ùå Failed'] = FailedRuns,\n    ['üìà Success Rate (%)'] = SuccessRate,\n    ['‚è±Ô∏è Avg Duration (min)'] = round(AvgDuration, 2)",
                    "size": 0,
                    "aggregation": 5,
                    "queryType": 0,
                    "resourceType": "microsoft.operationalinsights/workspaces",
                    "crossComponentResources": [
